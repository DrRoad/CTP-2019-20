function [hdrlum, ldrlum ldrchroma] = LoadFromID(ID)
    image = hdrread(strcat('../Output/Images/',strcat(ID,'.hdr'))); 
    ycbcr = rgb2ycbcr(image); 
    hdrlum = ycbcr(:,:,1);
    
    image = imread(strcat('../Output/Images/',strcat(STREETVIEW_ID,'_shifted.jpg')));
    ycbcr = rgb2ycbcr(image);
    ldrlum = ycbcr(:,:,1);
    ldrchroma = ycbcr(:, :, 2 : 3);
end

